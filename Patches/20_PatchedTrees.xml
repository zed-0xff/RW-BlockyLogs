<!-- trees patched by "Blocky Trees" mod, maybe other mods too -->
<Patch>
  <Operation Class="PatchOperationConditional">
    <xpath>Defs/ThingDef[label="mangrove tree"]</xpath>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationRemove">
          <xpath>Defs/ThingDef[label="mangrove tree"]/plant/harvestedThingDef</xpath>
          <success>Always</success>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/ThingDef[label="mangrove tree"]/plant</xpath>
          <value>
            <harvestedThingDef>WoodLog_Mangrove</harvestedThingDef>
          </value>
        </li>
      </operations>
    </match>
  </Operation>

