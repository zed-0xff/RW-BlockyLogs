<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<ThingDef ParentName="WoodLumberBase">
		<defName>LumberDarkOak</defName>
		<label>dark oak lumber</label>
		<description>Processed dark oak lumber. Mostly useful for building long-lasting, simple structures and furniture. Dark oak is a very durable hardwood.</description>
		<graphicData>
      <color>(95,61,29)</color>
		</graphicData>
		<statBases>
      <Flammability>0.5</Flammability>
			<Mass>0.5</Mass>
			<MarketValue>4</MarketValue>
			<DeteriorationRate>1</DeteriorationRate>
		</statBases>
		<stuffProps>
			<stuffAdjective>dark oak</stuffAdjective>
			<commonality>0.4</commonality>
      <color>(95,61,29)</color>
			<statFactors>
				<MaxHitPoints>0.75</MaxHitPoints>
				<Beauty>2.35</Beauty>
				<WorkToMake>0.6</WorkToMake>
				<WorkToBuild>0.6</WorkToBuild>
				<DoorOpenSpeed>1.0</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
  </ThingDef>

	<ThingDef ParentName="WoodLumberBase">
		<defName>LumberAcacia</defName>
		<label>acacia lumber</label>
		<description>Processed acacia lumber. Mostly useful for building simple structures, tools, and simple prosthetics. Acacia is a nondurable to perishable hardwood.</description>
		<graphicData>
      <color>(238,127,71)</color>
		</graphicData>
		<statBases>
			<Mass>0.15</Mass>
			<MarketValue>1.6</MarketValue>
			<DeteriorationRate>10</DeteriorationRate>
		</statBases>
		<stuffProps>
			<stuffAdjective>acacia</stuffAdjective>
			<commonality>0.4</commonality>
      <color>(238,127,71)</color>
			<statFactors>
				<MaxHitPoints>0.3</MaxHitPoints>
				<Beauty>3.3</Beauty>
				<WorkToMake>0.2</WorkToMake>
				<WorkToBuild>0.2</WorkToBuild>
				<DoorOpenSpeed>1.5</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
  </ThingDef>

	<ThingDef ParentName="WoodLumberBase">
		<defName>LumberMangrove</defName>
		<label>mangrove lumber</label>
		<description>Processed mangrove lumber. Mostly useful for building makeshift structures, handicrafts and furniture. Mangrove is a low durability hardwood.</description>
		<graphicData>
      <color>(166,78,68)</color>
		</graphicData>
		<statBases>
			<Mass>0.25</Mass>
			<MarketValue>1.3</MarketValue>
			<DeteriorationRate>6</DeteriorationRate>
		</statBases>
		<stuffProps>
			<stuffAdjective>mangrove</stuffAdjective>
			<commonality>0.4</commonality>
      <color>(166,78,68)</color>
			<statFactors>
				<MaxHitPoints>0.4</MaxHitPoints>
				<Beauty>3.15</Beauty>
				<WorkToMake>0.3</WorkToMake>
				<WorkToBuild>0.3</WorkToBuild>
				<DoorOpenSpeed>1.2</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
	</ThingDef>
</Defs>
