    <success>Always</success>
    <xpath>Defs/ThingDef[defName="WoodLog_Mangrove"]/stuffProps/color</xpath>
    <value>
      <color>(166,78,68)</color>
    </value>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <success>Always</success>
    <xpath>Defs/ThingDef[defName="WoodLog_Birch"]/graphicData</xpath>
